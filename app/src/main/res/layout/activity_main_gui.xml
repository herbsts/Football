<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_gui"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainGui">

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/statistics"
        android:id="@+id/imgbtnstatistics"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imgbtncalendar"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintRight_toRightOf="@+id/imgbtncalendar"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/profile"
        android:id="@+id/imgbtnprofile"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imgbtncalendar"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imgbtncalendar" />

    <ImageButton
        android:layout_width="193dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/calendar"
        android:id="@+id/imgbtncalendar"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgbtnteam"
        app:layout_constraintRight_toLeftOf="@+id/imgbtnteam"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="0dp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="259dp"
        app:srcCompat="@drawable/position"
        android:id="@+id/imgbtnteam"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/imgbtnstatistics"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imgbtnprofile"
        app:layout_constraintVertical_bias="0.454" />
</android.support.constraint.ConstraintLayout>
