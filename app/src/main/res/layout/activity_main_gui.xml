<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_gui"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:screenOrientation="portrait"
    tools:context=".MainGui">

    <GridLayout
        android:layout_width="396dp"
        android:layout_height="587dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp">

        <ImageButton
            android:id="@+id/imgbtnstatistics"
            android:layout_width="200dp"
            android:layout_height="300dp"
            app:srcCompat="@drawable/statistics"
            tools:layout_editor_absoluteX="230dp"
            tools:layout_editor_absoluteY="511dp" />

        <ImageButton
            android:id="@+id/imgbtncalendar"
            android:layout_width="200dp"
            android:layout_height="300dp"
            android:layout_column="0"
            android:layout_row="1"
            app:srcCompat="@drawable/calendar"
            tools:layout_editor_absoluteX="70dp"
            tools:layout_editor_absoluteY="521dp" />

        <ImageButton
            android:id="@+id/imgbtnteam"
            android:layout_width="200dp"
            android:layout_height="300dp"
            android:layout_column="1"
            android:layout_row="1"
            app:srcCompat="@drawable/position"
            tools:layout_editor_absoluteX="285dp"
            tools:layout_editor_absoluteY="530dp" />

        <ImageButton
            android:id="@+id/imgbtnprofile"
            android:layout_width="200dp"
            android:layout_height="300dp"
            android:layout_column="1"
            android:layout_row="0"
            app:srcCompat="@drawable/profile"
            tools:layout_editor_absoluteX="439dp"
            tools:layout_editor_absoluteY="349dp" />
    </GridLayout>

</android.support.constraint.ConstraintLayout>

